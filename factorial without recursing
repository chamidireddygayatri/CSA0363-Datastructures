#include<stdio.h>
int main()
{
	int n,i,fact=1;
	printf("Enter any number: ");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
	fact=fact*i;
    }
    printf("Factorial of %d = %d",n,fact);
return 0;
}

output
Enter any number: 12
Factorial of 12 = 479001600
--------------------------------
Process exited after 2.968 seconds with return value 0
